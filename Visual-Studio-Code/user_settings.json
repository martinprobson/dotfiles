    "editor.fontFamily": "'Source Code Pro'",
    "editor.fontSize": 16,
    "window.zoomLevel": 0,
    "terminal.external.linuxExec": "terminator",
    "terminal.explorerKind": "external",
    "editor.minimap.enabled": false,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["<leader>","e"],
            "after": [],
